{
    "name": "ᴀɴᴏɴ ᴍᴜsɪᴄ",
    "description": "ᴍᴜsɪᴄ ᴀʟʟᴏᴡ ʏᴏᴜ ᴛᴏ sᴛʀᴇᴀᴍ ᴍᴜsɪᴄ ᴛʜʀᴏᴜɢʜ ᴛʜᴇ ᴛᴇʟᴇɢʀᴀᴍ ᴠᴏɪᴄᴇ ᴄʜᴀᴛ.",
    "keywords": ["Anon", "AnonMusic", "music", "voice chat", "telegram", "Private Music", "telethon"],
    "repository": "https://github.com/Sumit0045/AnonMusic",
  "stack": "container",
    "env": {
        "STRING_SESSION": {
            "description": "fill with the telethon session string from https://replit.com/@Itz-zaid/Generator",
            "required": true
        },
        "HEROKU_MODE": {
            "description": "Don't Change this!",
            "required": true,
            "value": "ENABLE"
        },
        "SUPPORT": {
            "description": "Fill there your group username else leave",
            "required": false,
            "value": "Hindi_English_Chatt"
        },
        "CHANNEL": {
            "description": "Fill there your Channel username else leave",
            "required": false,
            "value": "TheBotUpdate"
        },
        "BOT_USERNAME": {
            "description": "Fill there your bot username",
            "required": false,
            "value": "AnonMusicBot"
        },
        "ASSISTANT_ID": {
            "description": "Fill there your Assistant id",
            "required": true,
            "value": ""
        },
        "BOT_TOKEN": {
            "description": "fill with your bot token get from @BotFather on telegram",
            "required": true,
            "value": ""
        }

    }
  }
